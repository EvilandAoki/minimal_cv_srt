{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@sections/*": [
        "./src/components/sections/*"
      ],
      "@cv": [
        "./src/assets/cv.json"
      ]
    }
  }
}